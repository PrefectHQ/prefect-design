  "files": [
    "dist"
  ],
  "types": "./dist/types/src/index.d.ts",
  "devDependencies": {
    "@prefecthq/eslint-config": "1.0.15",
    "@types/node": "^17.0.31",
    "@types/tailwindcss": "^3.0.10",
    "@vitejs/plugin-vue": "^2.3.1",
    "autoprefixer": "^10.4.6",
    "eslint": "^8.6.0",
    "postcss": "^8.4.13",
    "tailwindcss": "^3.0.24",
    "ts-node": "^10.7.0",
    "tsc-alias": "1.6.7",
    "typescript": "^4.6.4",
    "vite": "^2.9.6",
    "vite-svg-loader": "3.3.0",
    "vue-tsc": "0.34.11"
  },
  "peerDependencies": {
    "vue": "^3.0.5"
