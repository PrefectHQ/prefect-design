{
  "root": true,
  "env": {
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 2020,
    "project": ["./tsconfig.json"],
    "sourceType": "module",
    "parser": "@typescript-eslint/parser"
  },
  "extends": [
    "@prefecthq"
  ],
  "globals": {
    "defineProps": "readonly",
    "defineEmits": "readonly",
    "defineExpose": "readonly",
    "defineModel": "readonly"
  },
  "settings": {
    "import/resolver": {
      "typescript": {
        "project": "./tsconfig.json"
      }
    }
  },
  "rules": {
    "import/no-default-export": "off"
  }
}