{
  "name": "@prefecthq/prefect-design",
  "workspaces": [
    "./demo"
  ],
  "version": "1.1.29",
  "description": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PrefectHQ/prefect-design.git"
  },
  "keywords": [
    "vue",
    "vue3",
    "typescript",
    "prefect"
  ],
  "scripts": {
    "serve": "vite --host --mode=demo",
    "dev": "vue-tsc -w & tsc-alias -w & vite build -w",
    "build": "vue-tsc && tsc-alias && vite build",
    "build:demo": "vite build --mode=demo",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "changelog": "auto-changelog --package --commit-limit 0",
    "changelog:commit": "npm run changelog && git add -A && git commit -m 'Update CHANGELOG.md'",
    "release:patch": "npm version patch && npm run changelog:commit",
    "release:minor": "npm version minor && npm run changelog:commit",
    "release:major": "npm version major && npm run changelog:commit'"
  },
  "main": "./dist/prefect-design.umd.js",
  "module": "./dist/prefect-design.es.js",
  "exports": {
    ".": {
      "import": "./dist/prefect-design.es.js",
      "require": "./dist/prefect-design.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "types": "./dist/types/src/index.d.ts",
  "devDependencies": {
    "@fontsource/inconsolata": "^4.5.7",
    "@fontsource/inter": "^4.5.12",
    "@prefecthq/eslint-config": "1.0.17",
    "@prefecthq/vue-compositions": "0.2.6",
    "@types/node": "^18.7.13",
    "@types/tailwindcss": "^3.0.10",
    "@vitejs/plugin-vue": "^2.3.1",
    "auto-changelog": "^2.4.0",
    "autoprefixer": "^10.4.6",
    "eslint": "^8.6.0",
    "postcss": "^8.4.13",
    "tailwindcss": "^3.0.24",
    "ts-node": "^10.7.0",
    "tsc-alias": "1.7.0",
    "typescript": "4.8.4",
    "vite": "^2.9.6",
    "vite-svg-loader": "3.6.0",
    "vue-router": "4.1.6",
    "vue-tsc": "1.0.9"
  },
  "dependencies": {
    "@heroicons/vue": "^1.0.6",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.0",
    "@tailwindcss/typography": "^0.5.2",
    "@types/lodash.debounce": "4.0.7",
    "date-fns": "2.29.3",
    "lodash.debounce": "4.0.8",
    "vue": "^3.2.41",
    "vue-router": "^4.0.12"
  }
}
